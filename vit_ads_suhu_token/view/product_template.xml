<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_product_template_form_ai_token" model="ir.ui.view">
            <field name="name">product.template.form.ai.token</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet//group" position="after">
                    <group string="AI Token">
                        <field name="ai_token_qty"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
