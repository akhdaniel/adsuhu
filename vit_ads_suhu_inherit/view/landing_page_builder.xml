<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- tree view -->
        <record id="view_landing_page_builder_tree" model="ir.ui.view">
            <field name="name">landing_page_builder_tree</field>
            <field name="model">vit.landing_page_builder</field>
            <field name="inherit_id" ref="vit_ads_suhu.view_vit_landing_page_builder_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//list" position="attributes">
                    <attribute name="js_class">ads_list_with_banner</attribute>
                </xpath>            
            </field>
        </record>
        <!-- form view -->
        <record id="view_landing_page_builder_form" model="ir.ui.view">
            <field name="name">landing_page_builder_form</field>
            <field name="model">vit.landing_page_builder</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="vit_ads_suhu.view_vit_landing_page_builder_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="attributes">
                    <attribute name="js_class">ads_form_with_banner</attribute>
                </xpath>             
            </field>
        </record>
